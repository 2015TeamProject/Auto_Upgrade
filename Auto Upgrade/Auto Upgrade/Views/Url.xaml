<Window x:Class="Auto_Upgrade.Url"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Auto_Upgrade"
        mc:Ignorable="d"
        Title="设置URL" Height="200" Width="600">
    <Grid>
        <Label x:Name="label" Content="Default url:" HorizontalAlignment="Left" Height="35" Margin="12,49,0,0" VerticalAlignment="Top" Width="108" FontSize="18"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="34" Margin="119,50,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="446" FontSize="10" Cursor="Hand" MaxLines="1" />
        <Button x:Name="confirmButton" Content="确定" HorizontalAlignment="Left" Height="24" Margin="185,111,0,0" VerticalAlignment="Top" Width="89" Click="ConfirmButton_Click"/>
        <Button x:Name="cancelButton" Content="取消" HorizontalAlignment="Left" Margin="349,111,0,0" VerticalAlignment="Top" Width="89" Height="24" Click="CancelButton_Click"/>
    </Grid>
</Window>
