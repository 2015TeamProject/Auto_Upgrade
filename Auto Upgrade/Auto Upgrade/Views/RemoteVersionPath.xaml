<Window x:Class="Auto_Upgrade.Views.RemoteVersionPath"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Auto_Upgrade"
        mc:Ignorable="d"
        Title="远端版本目标文件URL" Height="200" Width="500">
    <Grid>
        <Button x:Name="confirmButton" Content="确定" HorizontalAlignment="Left" Margin="156,118,0,0" VerticalAlignment="Top" Width="75" Click="Confirm_Click"/>
        <Button x:Name="cancelButton" Content="取消" HorizontalAlignment="Left" Margin="286,118,0,0" VerticalAlignment="Top" Width="75" Click="Cancel_Click"/>
        <TextBox x:Name="url" HorizontalAlignment="Left" Height="23" Margin="125,66,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="344"/>
        <Label x:Name="urlPath" Content="目标文件的URL: " HorizontalAlignment="Left" VerticalAlignment="Top" Margin="26,66,0,0"/>

    </Grid>
</Window>
